{
  "name": "ludo-3d",
  "version": "0.1.0",
  "scripts": {
    "start-client": "webpack-dev-server --open --config webpack.config.dev.js",
    "start-server": "nodemon --ext ts --watch src/ --exec ts-node src/server.ts",
    "start": "concurrently --kill-others \"npm run start-server\" \"npm run start-client\"",
    "build": "webpack --config webpack.config.prod.js --progress"
  },
  "author": "Borut Balazek",
  "dependencies": {
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "babylonjs": "^4.0.3",
    "babylonjs-loaders": "^4.0.3",
    "babylonjs-materials": "^4.0.3",
    "colyseus": "^0.14.24",
    "colyseus.js": "^0.10.4",
    "dotenv": "^7.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-keycode-enum": "^1.0.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^5.1.2",
    "nodemon": "^2.0.20",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^4.11.1",
    "webpack-dotenv-plugin": "^2.1.0",
    "webpack-merge": "^4.2.1"
  }
}
